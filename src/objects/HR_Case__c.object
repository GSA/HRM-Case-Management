        <errorConditionFormula>AND 
(AND
(NOT(ISPICKVAL( HRM_RA_Action_Appealed__c ,&quot;Yes&quot;)), ISBLANK(HRM_RA_Date_Reconsideration_Received__c)
), 
ISPICKVAL( Status__c ,&quot;Closed - In Appeal&quot;), 
RecordType.DeveloperName  = &apos;HRM_Reasonable_Accommodation_RA_Case&apos;)</errorConditionFormula>
        <errorMessage>&quot;Action Appealed&quot; should be &quot;Yes&quot; OR &quot;Date Reconsideration Received&quot; should be filled in before status of the Case can be changed to &quot;Closed – In Appeal&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_AO_IsNotNull</fullName>
        <active>true</active>
        <description>ROO/Action Officer field cannot be blank from Assigned to Responsible Office Official stage onwards</description>
        <errorConditionFormula>AND (RecordType.DeveloperName  = &apos;HRM_OIG_Case&apos;, OR(ISPICKVAL(Stage__c,&quot;Assigned to Responsible Office Official&quot;),ISPICKVAL(Stage__c,&quot;Assigned to Action Officer&quot;), ISPICKVAL(Stage__c,&quot;Assigned to Assigned Specialist&quot;),ISPICKVAL(Stage__c,&quot;Ready for Action Officer to View&quot;), ISPICKVAL(Stage__c,&quot;Reviewed by Action Officer&quot;)), ISBLANK( HRM_ROO_AO__c) )</errorConditionFormula>
        <errorDisplayField>HRM_ROO_AO__c</errorDisplayField>
        <errorMessage>ROO/Action Officer field cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_AS_IsNotNull</fullName>
        <active>true</active>
        <description>Assigned Specialist field cannot be blank from Assigned to Assigned Specialist stage onward</description>
        <errorConditionFormula>AND (RecordType.DeveloperName  = &apos;HRM_OIG_Case&apos;, OR(ISPICKVAL(Stage__c,&quot;Assigned to Assigned Specialist&quot;),ISPICKVAL(Stage__c,&quot;Ready for Action Officer to View&quot;)), ISBLANK( HRM_Assigned_Specialist__c ) )</errorConditionFormula>
        <errorDisplayField>HRM_Assigned_Specialist__c</errorDisplayField>
        <errorMessage>Assigned Specialist field cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_AS_Office_IsNotNull</fullName>
        <active>true</active>
        <description>HR Office of Assigned Specialist field cannot be blank when an Assigned Specialist is selected.</description>
        <errorConditionFormula>AND (NOT(ISBLANK(HRM_Assigned_Specialist__c)), ISPICKVAL( HRM_Office_of_Assigned_Specialist__c,&quot;&quot; ) )</errorConditionFormula>
        <errorDisplayField>HRM_Office_of_Assigned_Specialist__c</errorDisplayField>
        <errorMessage>HR Office of Assigned Specialist field cannot be blank when an Assigned Specialist is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Deleted_Records</fullName>
        <active>true</active>
        <description>AND (OR(ISCHANGED(Stage__c), ISCHANGED(Status__c)), Deleted__c )</description>
        <errorConditionFormula>AND (RecordType.DeveloperName  = &apos;HRM_OIG_Case&apos;, OR(ISCHANGED(Stage__c), ISCHANGED(Status__c)), Marked_for_Deletion__c)</errorConditionFormula>
        <errorMessage>Cannot change Status or Stage for records marked as Deleted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_AO_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISCHANGED(Stage__c), ISPICKVAL( Stage__c ,&quot;Assigned to Action Officer&quot;), NOT(OR(ISPICKVAL(PRIORVALUE( Stage__c ), &quot;With Intake Reviewer&quot;), ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Assigned to Responsible Office Official&quot;))))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move to &quot;Assigned to Action Officer&quot; stage from the current stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_AS_Stage_NonPrivate</fullName>
        <active>true</active>
        <description>Cannot move to Assigned to Assigned Specialist stage from stages other than Assigned to Action Officer and Ready for Action Officer to View</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISCHANGED(Stage__c), ISPICKVAL( Stage__c ,&quot;Assigned to Assigned Specialist&quot;), NOT( Private__c ), NOT(OR(ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Assigned to Action Officer&quot;),ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Ready for Action Officer to View&quot;))))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move to &quot;Assigned to Assigned Specialist&quot; stage from the current stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_AS_Stage_Private</fullName>
        <active>true</active>
        <description>For private cases, cannot move to Assigned to Assigned Specialist stage from stages other than Assigned to Responsible Office Official, Assigned to Action Officer and Ready for Action Officer to View</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISCHANGED(Stage__c), ISPICKVAL( Stage__c ,&quot;Assigned to Assigned Specialist&quot;), Private__c, NOT(OR(ISPICKVAL(PRIORVALUE( Stage__c ), &quot;With Intake Reviewer&quot;),ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Assigned to Responsible Office Official&quot;),ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Assigned to Action Officer&quot;))))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move to &quot;Assigned to Assigned Specialist&quot; stage from the current stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_Closed_In_Appeal_Sta</fullName>
        <active>true</active>
        <description>Status cannot be changed from &quot;Open&quot; to &quot;Closed - In Appeal&quot;.</description>
        <errorConditionFormula>AND (OR(RecordType.DeveloperName = &apos;HRM_Disciplinary_Case&apos;, RecordType.DeveloperName = &apos;HRM_Performance_Case&apos;, RecordType.DeveloperName = &apos;HRM_Medical_Case&apos;, RecordType.DeveloperName =  &apos;HRM_Reasonable_Accommodation_RA_Case&apos;, RecordType.DeveloperName = &apos;HRM_Grievances_and_Arbitration_Case&apos; ), ISCHANGED(Status__c), 
ISPICKVAL( Status__c ,&quot;Closed - In Appeal&quot;), 
NOT(ISPICKVAL(PRIORVALUE( Status__c ), &quot;Closed&quot;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please close the case before moving Status to &quot;Closed - In Appeal&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_Closed_Status</fullName>
        <active>true</active>
        <description>Case status can be changed to Closed only from Ready for Action Officer to View stage</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;,ISCHANGED(Status__c), ISPICKVAL( Status__c ,&quot;Closed&quot;), NOT(ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Ready for Action Officer to View&quot;)))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move Status to &quot;Closed&quot; from the current stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_ROO_Stage</fullName>
        <active>true</active>
        <description>Cannot move back to Assigned to Responsible Office Official stage from the current stage</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISCHANGED(Stage__c), ISPICKVAL( Stage__c ,&quot;Assigned to Responsible Office Official&quot;), NOT(ISPICKVAL(PRIORVALUE( Stage__c ), &quot;With Intake Reviewer&quot;)))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move to &quot;Assigned to Responsible Office Official&quot; stage from the current stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Entry_Into_Ready_For_AO_Stage</fullName>
        <active>true</active>
        <description>Case can move to Ready for Action Officer to View stage only from Assigned to Assigned Specialist stage</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISCHANGED(Stage__c), ISPICKVAL( Stage__c ,&quot;Ready for Action Officer to View&quot;), NOT( Private__c ), NOT(ISPICKVAL(PRIORVALUE( Stage__c ), &quot;Assigned to Assigned Specialist&quot;)))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move to &quot;Ready for Action Officer to View&quot; stage from the current stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_New_Cases</fullName>
        <active>true</active>
        <description>New cases cannot go directly to Assigned to Assigned Specialist or Ready for Action Officer to View stage.  A ROO/AO should be assigned first.</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISNEW(), OR(ISPICKVAL( Stage__c ,&quot;Assigned to Assigned Specialist&quot;), ISPICKVAL( Stage__c ,&quot;Ready for Action Officer to View&quot;)))</errorConditionFormula>
        <errorDisplayField>HRM_ROO_AO__c</errorDisplayField>
        <errorMessage>New cases cannot go directly to the selected stage.  A ROO/AO should be assigned first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_With_Intake_Reviewer_Stage</fullName>
        <active>true</active>
        <description>Case cannot go back to With Intake Reviewer stage</description>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;HRM_OIG_Case&apos;, ISCHANGED(Stage__c),ISPICKVAL(Stage__c, 
&quot;With Intake Reviewer&quot;), NOT(ISNEW()))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Cannot move to &quot;With Intake Reviewer&quot; stage from the current stage.</errorMessage>
    </validationRules>
